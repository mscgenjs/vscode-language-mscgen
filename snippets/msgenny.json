{
    "MsGenny skeleton": {
        "prefix": "msc",
        "body": [
            "# options",
            "wordwraparcs=on;",
            "",
            "# entities",
            "${1:a}: \"${2:Sender}\";",
            "${3:b}:\"${4:Receiver}\";",
            "",
            "# arcs",
            "${1:a} =>> ${3:b} \"Message\";",
            "${3:b} >> ${1:a} \"Response\";",
            "$5"
        ],
        "description" : "MsGenny function skeleton"
    },
    "MsGenny entity": {
        "prefix": "ent",
        "body": [
            "${1:entity}: \"${2:Entity}\"$3"
        ],
        "description": "MsGenny entity"
    },
    "MsGenny arc": {
        "prefix": "arc",
        "body": [
            "${1:from} =>> ${2:to}: \"${3:Message}\";"
        ],
        "description": "MsGenny arc/ message"
    },
    "MsGenny loop": {
        "prefix": "loop",
        "body": [
            "${1:from} loop ${2:to}: \"${3:For every ...}\" {",
            "  $4",
            "}"
        ],
        "description": "MsGenny loop"
    },
    "MsGenny alt": {
        "prefix": "alt",
        "body": [
            "${1:from} alt ${2:to}: \"${3:Happy day}\" {",
            "  $5",
            "  ---: "${4:Alternative scenario}";",
            "}"
        ],
        "description": "MsGenny alt"
    }
}